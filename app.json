{
  "expo": {
    "name": "OnMe",
    "slug": "OnMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets//OnMeLogo.png",
    "splash": {
      "image": "./assets/OnMeSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F2D2D"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "OnMe"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
